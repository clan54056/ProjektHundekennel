
<Window
    x:Class="ProjektHundekennel.Views.UploadToDatabase"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:ProjektHundekennel.Views"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    Title="UploadToDatabase"
    Width="500"
    Height="250"
    mc:Ignorable="d">
    <Grid>

        <Button
            Width="151"
            Height="32"
            Margin="243,99,0,0"
            HorizontalAlignment="Left"
            VerticalAlignment="Top"
            Click="HandleUploadToDatabase"
            Content="Vælg excel fil" />
        <Label
            Margin="90,101,0,0"
            HorizontalAlignment="Left"
            VerticalAlignment="Top"
            Content="Indlæs data fra excel fil" />

        <ListBox x:Name="destination" d:ItemsSource="{d:SampleData ItemCount=5}" Margin="10,33,255,0" BorderThickness="0" Height="NaN" Width="NaN" FontFamily="Bahnschrift" FontSize="20"/>
//en liste med uploads?=======



    </Grid>
</Window>
